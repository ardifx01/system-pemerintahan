{
    "compilerOptions": {
        "target": "ESNext",
        "baseUrl": ".",
        "paths": {
            "@/*": ["resources/js/*"]
        },
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "strict": true,
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "include": [
        "resources/js/**/*.ts",
        "resources/js/**/*.d.ts",
        "resources/js/**/*.tsx"
    ]
}
